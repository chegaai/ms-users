{
  "name": "ms-users",
  "version": "0.1.0",
  "description": "Users microservice",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build:clean",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "test": "exit 0",
    "coverage": "exit 0",
    "lint": "tslint --project tsconfig.json",
    "build": "tsc",
    "build:debug": "npm run clean && npm run build -- --sourceMap",
    "build:clean": "npm run clean && npm run build",
    "build:watch": "tsc -w",
    "start": "node dist/index.js",
    "start:debug": "tsc && concurrently -s all \"tsc -w\" \"DEBUG=expresso:* nodemon dist/index.js\""
  },
  "contributors": [
    "Lucas Santos <hello@lsantos.dev>"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@expresso/app": "^1.2.2",
    "@expresso/errors": "^1.2.0",
    "@expresso/server": "^1.1.3",
    "@expresso/validator": "^1.1.1",
    "@nindoo/mongodb-data-layer": "^2.1.0",
    "@types/jsonwebtoken": "^8.3.4",
    "axios": "^0.19.0",
    "express-rescue": "1.1.20",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "sugar-env": "^1.5.9",
    "tsyringe": "^3.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.22",
    "concurrently": "^4.1.1",
    "husky": "^3.1.0",
    "nodemon": "^1.19.1",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-standard": "^8.0.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.7.4"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run build:clean"
    }
  }
}
